<Project>
	<PropertyGroup>
		<!--
		<TargetFramework>net9.0</TargetFramework>
		<TargetFrameworks>net9.0;net10.0</TargetFrameworks>
		-->

		<Nullable>enable</Nullable>
		
		<NoWarn>$(NoWarn);1998</NoWarn> <!-- no await in async -->
		<NoWarn>$(NoWarn);IL2026;IL3050;</NoWarn> <!-- STJ Gen -->

		<WarningsAsErrors>$(WarningsAsErrors);8785</WarningsAsErrors> <!-- generator failed -->
		
		<LangVersion>latest</LangVersion>
		<!--
		<EnableMicrosoftExtensionsConfigurationBinderSourceGenerator>true</EnableMicrosoftExtensionsConfigurationBinderSourceGenerator> < ! - - Preview 3 - - >
		<EnableConfigurationBindingGenerator>true</EnableConfigurationBindingGenerator> < ! - - Preview 4 - - >
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		-->
		<IsAotCompatible Condition="$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net8.0'))">true</IsAotCompatible>
		<!--
		<IsAotCompatible Condition="$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net7.0')) and $(TargetFramework) != 'netstandard2.0' and $([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net48'))">true</IsAotCompatible>
		<IsAotCompatible Condition="$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net9.0'))">true</IsAotCompatible>
		<IsAotCompatible Condition="$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net10.0'))">true</IsAotCompatible>
		-->
	</PropertyGroup>
</Project>
